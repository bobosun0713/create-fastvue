    "@tailwindcss/nesting": "0.0.0-insiders.565cd3e",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.14.12",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-vue": "^5.0.5",
