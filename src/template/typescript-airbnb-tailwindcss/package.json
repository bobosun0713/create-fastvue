    "jsdom": "^26.0.0",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.45",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.4.5",
